{
  "name": "bubble-sort",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/index.js",
  "scripts": {
    "clean": "npm run clean:dist && npm run clean:gen",
    "clean:dist": "rm -rf dist",
    "clean:gen": "rm -rf gen",
    "build": "npm run clean && npm run gen && tsc",
    "gen": "npm run gen:proto",
    "gen:proto": "mkdir -p gen/grpc && protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_opt=outputServices=nice-grpc,outputServices=generic-definitions,useExactTypes=false,esModuleInterop=true,env=node,stringEnums=true --ts_proto_out=./gen/grpc --proto_path=../protos ../protos/sort.proto",
    "lint": "eslint --ext .ts --max-warnings 0 src/",
    "lint:fix": "npm run lint -- --fix",
    "start": "node dist/src/index.js",
    "dev": "nodemon",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivy-gmbh/bubble-benchmark.git"
  },
  "author": "Matt de Young",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ivy-gmbh/bubble-benchmark/issues"
  },
  "homepage": "https://github.com/ivy-gmbh/bubble-benchmark#readme",
  "dependencies": {
    "dotenv": "^16.0.3",
    "nice-grpc": "^2.0.1",
    "nice-grpc-server-health": "^2.0.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "ts-proto": "^1.131.0",
    "typescript": "^4.8.4"
  }
}
